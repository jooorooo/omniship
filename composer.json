{
  "name": "simexis/omniship",
  "type": "library",
  "description": "Common components for Omniship shipping processing library",
  "keywords": [
    "gateway",
    "omniship",
    "ship",
    "shipping"
  ],
  "homepage": "https://github.com/jooorooo/omniship",
  "license": "MIT",
  "authors": [
    {
      "name": "Georgi Nachev",
      "email": "jooorooo@gmail.com"
    }
  ],
  "require": {
    "php": "^7",
    "guzzlehttp/guzzle": "^6.3@dev",
    "php-http/httplug": "^2.0@dev",
    "php-http/message-factory": "^1.0@dev",
    "php-http/discovery": "^1.5@dev",
    "php-http/message": "^1.6@dev",
    "php-http/curl-client": "^2.0@dev",
    "symfony/intl": "^2.1|^3.1|^4.3@dev",
    "symfony/http-foundation": "^2.1|^3.1|^4.1",
    "commerceguys/addressing": "^0.8.4",
    "crisu83/php-conversion": "^0.5|dev-master",
    "nesbot/carbon": "~1.20|dev-master",
    "guzzlehttp/psr7": "^1.5@dev"
  },
  "require-dev": {
    "php-http/mock-client": "^1",
    "squizlabs/php_codesniffer": "^2.8.1"
  },
  "autoload": {
    "psr-4": {
      "Omniship\\": "src"
    },
    "files": [
      "src/Helper/helpers.php"
    ]
  },
  "suggest": {
    "php-http/guzzle6-adapter": "Use the Guzzle adapter to send your requests."
  },
  "scripts": {
    "check-style": "phpcs -p --standard=PSR2 src/",
    "fix-style": "phpcbf -p --standard=PSR2 src/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
